{
  "name": "jsreport-scripts",
  "version": "0.1.0",
  "description": "jsreport extension capable of running custom javascript functions during the rendering process",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "prepublish": "node node_modules/jsreport-express/tools/optimize.js"
  },
  "homepage": "https://github.com/jsreport/jsreport-scripts",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-scripts.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "define",
      "ace"
    ]
  },
  "license": "LGPL",
  "dependencies": {
    "feedparser": "1.1.4",
    "moment": "2.10.6",
    "q": "1.4.1",
    "sendgrid": "1.9.2",
    "shortid": "2.2.4",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "^1.9.0",
    "jsreport-core": "*",
    "jsreport-express": "*",
    "jsreport-jsrender": "*",
    "jsreport-templates": "*",
    "mocha": "^2.3.3",
    "requirejs": "^2.1.22",
    "should": "^7.1.1",
    "standard": "^5.3.1",
    "string": "^3.3.1"
  }
}
